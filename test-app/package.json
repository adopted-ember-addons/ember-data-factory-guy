{
  "name": "test-app",
  "version": "4.0.0",
  "description": "Factories for testing Ember applications using EmberData",
  "private": true,
  "keywords": [
    "testing",
    "factories",
    "ember-data"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-data-factory-guy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adopted-ember-addons/ember-data-factory-guy.git",
    "directory": "test-app"
  },
  "license": "MIT",
  "author": "Daniel Sudol <dansudol@yahoo.com>",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "test": "ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:js": "pnpm -w eslint ./test-app",
    "lint:js:fix": "pnpm -w eslint ./test-app --fix",
    "lint:hbs": "pnpm -w ember-template-lint ./test-app"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^5.2.1",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/macros": "^1.17.2",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "active-model-adapter": "^4.0.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-data": "~5.3",
    "ember-data-factory-guy": "workspace:*",
    "ember-inflector": "^6.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^4.3.0",
    "ember-try": "^4.0.0",
    "loader.js": "^4.7.0",
    "pretender": "^3.4.7",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "sinon": "^20.0.0",
    "webpack": "^5.96.1"
  },
  "engines": {
    "node": "18.* || 20.* || >= 22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  }
}
