{
  "name": "test-app",
  "version": "4.0.0",
  "description": "Factories for testing Ember applications using EmberData",
  "private": true,
  "keywords": [
    "testing",
    "factories",
    "ember-data"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-data-factory-guy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adopted-ember-addons/ember-data-factory-guy.git",
    "directory": "test-app"
  },
  "license": "MIT",
  "author": "Daniel Sudol <dansudol@yahoo.com>",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "build": "ember build --environment=production",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.4.2",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/macros": "^1.17.2",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "active-model-adapter": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~3.28.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-data": "~3.28.13",
    "ember-data-factory-guy": "workspace:*",
    "ember-data-model-fragments": "^5.0.0-beta.2",
    "ember-inflector": "^6.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "5.0.0",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.7",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.96.1"
  },
  "engines": {
    "node": "18.* || 20.* || >= 22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  }
}
