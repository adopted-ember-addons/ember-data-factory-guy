{
  "name": "ember-data-factory-guy",
  "version": "4.0.0-alpha.2",
  "description": "Factories for testing Ember applications using EmberData",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "author": "Daniel Sudol <dansudol@yahoo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-data-factory-guy"
  },
  "homepage": "https://github.com/adopted-ember-addons/ember-data-factory-guy",
  "scripts": {
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "build": "ember build --environment=production",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "dependencies": {
    "broccoli-funnel": "^3.0.4",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.7.4",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "pretender": "^3.4.3",
    "resolve": "^1.20.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.1.1",
    "ember-inflector": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.0.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-test-selectors": "^6.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "sinon": "^18.0.0",
    "webpack": "^5.93.0"
  },
  "peerDependencies": {
    "@glimmer/component": ">= 1.1.2",
    "@glimmer/tracking": ">= 1.1.2",
    "ember-data": "^3.24.0 || ^4.0.0 || >= 5.0.0",
    "ember-source": "^3.28.0 || ^4.0.0 || >= 5.0.0"
  },
  "keywords": [
    "ember-addon",
    "testing",
    "factories",
    "ember-data"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "main": "index.js",
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
