{
  "name": "ember-data-factory-guy",
  "version": "5.1.0",
  "description": "Factories for testing Ember applications using EmberData",
  "keywords": [
    "ember-addon",
    "testing",
    "factories",
    "ember-data"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-data-factory-guy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adopted-ember-addons/ember-data-factory-guy.git",
    "directory": "addon"
  },
  "license": "MIT",
  "author": "Daniel Sudol <dansudol@yahoo.com>",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:js": "pnpm -w eslint ./addon",
    "lint:js:fix": "pnpm -w eslint ./addon --fix"
  },
  "exports": {
    ".": "./dist/index.js",
    "./-private": "./dist/-private.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "dependencies": {
    "@embroider/addon-shim": "^1.10.0",
    "pretender": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@ember-data/model": "^3.28.13",
    "@ember-data/serializer": "^3.28.13",
    "@ember-data/store": "^3.28.13",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/addon-dev": "^8.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "active-model-adapter": "^3.0.1",
    "ember-data-model-fragments": "^6.0.10",
    "ember-inflector": "^4.0.1",
    "rollup": "^4.40.1",
    "webpack": "^5.96.1"
  },
  "peerDependencies": {
    "@ember-data/model": ">= 3.28",
    "@ember-data/serializer": ">= 3.28",
    "@ember-data/store": ">= 3.28",
    "@ember/string": ">= 3.0.0",
    "@ember/test-helpers": "^2.9.3 || >= 3",
    "active-model-adapter": "^3.0.1 || ^4.0.0",
    "ember-data-model-fragments": "^5.0.0 || ^6.0.10",
    "ember-inflector": ">= 4.0.1"
  },
  "peerDependenciesMeta": {
    "active-model-adapter": {
      "optional": true
    },
    "ember-data-model-fragments": {
      "optional": true
    }
  },
  "engines": {
    "node": "18.* || 20.* || >= 22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2
  }
}
